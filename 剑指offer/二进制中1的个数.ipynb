    "# -*- coding:utf-8 -*-\n",
    "class Solution:\n",
    "    def NumberOf1(self, n):\n",
    "        # write code here\n",
    "        return bin(n).replace(\"0b\",\"\").count(\"1\") if n >= 0 else bin(2**32+n).replace(\"0b\",\"\").count(\"1\")"
   ]
  },
